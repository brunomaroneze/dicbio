dbres:entry_antera a ontolex:LexicalEntry ;
    dcterms:created "2022-02-28"^^xsd:date ;
    dcterms:creator dbauth:bruno_maroneze ;
    lexinfo:gender lexinfo:feminine ;
    lexinfo:partOfSpeech lexinfo:noun ;
    ontolex:canonicalForm dbres:form_antera ;
    ontolex:otherForm dbres:form_anteras,
        dbres:form_anthera,
        dbres:form_antheras,
        dbres:form_anthera_accent,
