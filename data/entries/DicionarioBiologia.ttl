    ontolex:writtenRep "filamentos"@pt .

dbres:entry_filamento_sense1 a ontolex:LexicalSense ;
    skos:definition "Estrutura que sustenta a antera e a une à planta."@pt ;
    etym:etymology dbres:etym_filamento_sense1 .

dbres:entry_filamento_sense2 a ontolex:LexicalSense ;
    skos:definition "Qualquer estrutura animal semelhante a um fio, como antenas ou bissos."@pt .

