### --- VERBETE: HIMEN ---

dbres:entry_himen a ontolex:LexicalEntry ;
    dcterms:created "2025-09-13"^^xsd:date ;
    dcterms:creator dbauth:ana_cristina_lopes,
        dbauth:bruno_maroneze ;
    lexinfo:gender lexinfo:masculine ;
    lexinfo:partOfSpeech lexinfo:noun ;
