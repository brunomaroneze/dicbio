    dcterms:language <http://www.lexvo.org/page/iso639-3/lat> ;
    dcterms:source "Verheyen (1710 - https://www.google.com.br/books/edition/Corporis_humani_anatomia/BCNgAAAAcAAJ" ;
    ontolex:writtenRep "excretorius" .

