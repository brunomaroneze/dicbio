    dcterms:language <http://www.lexvo.org/page/iso639-3/lat> ;
    dcterms:source "\"Anatomia\" de Domenico Marchetti, 1656 (https://www.google.com.br/books/edition/Anatomia/4fQGAAAAcAAJ)" ;
    ontolex:writtenRep "malleolus" .

