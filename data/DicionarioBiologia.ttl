    dcterms:date "1788" ;
    rdf:value """O primeiro termo he melhor, porque nos ramos ha às vezes
											[[b]]folhas[[/b]] que saõ
										patentissimas, e naõ saõ parallelas ao plano da terra ou
										horizontaes."""@pt .

dicbio:usage_folha_610 a ontolex:UsageExample ;
