    dcterms:date "1739" ;
    rdf:value """O [[b]]Diaphragma[[/b]] he de figura quasi
              redonda, e semelhante ao peixe, que se chama Arraya, a cauda da qual representa os
              seus processos, ou appendices."""@pt .

dicbio:usage_diafragma_298 a ontolex:UsageExample ;
