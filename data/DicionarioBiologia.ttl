    skos:definition "Ramo da História Natural que estuda os animais."@pt .

# 5 adiposo
dicbio:entry_adiposo a ontolex:LexicalEntry ;
    dcterms:created "2024-05-04"^^xsd:date ;
    dcterms:creator author:bruno_maroneze,
        author:fabiani_goncalves ;
    etym:etymology dicbio:etym_adiposo ;
    lexinfo:partOfSpeech lexinfo:adjective ;
    ontolex:canonicalForm dicbio:form_adiposo ;
    ontolex:otherForm dicbio:form_Adiposa,
        dicbio:form_adiposas,
        dicbio:form_adiposos ;
    ontolex:sense dicbio:sense_adiposo_1 .

dicbio:etym_adiposo a etym:Etymology ;
    etym:etymon dicbio:etymon_adiposus ;
    rdfs:comment "O dicionário Houaiss afirma tratar-se de derivação sufixal a partir de <i>ádipe</i> (gordura animal) com o acréscimo do sufixo <i>-oso</i>. No entanto, a forma latina <i>adiposus</i>, ainda que não esteja registrada nos dicionários de latim da Antiguidade, pode ser encontrada em textos em latim científico, como, por exemplo, na expressão “panniculus adiposus”, presente na “Acta Physico-Medica” de 1730 (https://www.google.com.br/books/edition/Acta_physico_medica_Academiae_caesareae/bYy3qY5Fgn8C). Dessa forma, o étimo da forma portuguesa pode ser o latim científico, e não uma formação vernacular, como propõe o dicionário Houaiss."@pt .

dicbio:etymon_adiposus a etym:Etymon ;
    dcterms:language "latim científico" ;
    dcterms:source "“panniculus adiposus”, presente na “Acta Physico-Medica” de 1730 (https://www.google.com.br/books/edition/Acta_physico_medica_Academiae_caesareae/bYy3qY5Fgn8C)" ;
    ontolex:writtenRep "adiposus" .

dicbio:form_adiposas a ontolex:Form ;
    rdfs:label "feminino plural"@pt ;
    ontolex:writtenRep "adiposas"@pt .

dicbio:form_adiposo a ontolex:Form ;
    ontolex:writtenRep "adiposo"@pt .

dicbio:form_adiposos a ontolex:Form ;
    rdfs:label "masculino plural"@pt ;
    ontolex:writtenRep "adiposos"@pt .

dicbio:sense_adiposo_1 a ontolex:LexicalSense ;
    skos:definition "Que contém gordura."@pt .

# 6 angulado
dicbio:entry_angulado a ontolex:LexicalEntry ;
    dcterms:created "2023-09-15"^^xsd:date ;
    dcterms:creator author:bruno_maroneze,
        author:kamila_barbosa ;
    etym:etymology dicbio:etym_angulado ;
    lexinfo:partOfSpeech lexinfo:adjective ;
    ontolex:canonicalForm dicbio:form_angulado ;
    ontolex:otherForm dicbio:form_Angulada,
        dicbio:form_Anguladas,
        dicbio:form_angulada,
        dicbio:form_angulados ;
    ontolex:sense dicbio:sense_angulado_1 .

    dicbio:etym_angulado a etym:Etymology ;
    etym:etymon dicbio:etymon_angulatus ;
    rdfs:comment "Duas são as possibilidades de descrição do étimo: 1 - <i>angulado</i> pode ter como étimo o latim <i>angŭlātus</i> (atestado na Antiguidade, conforme registrado nos dicionários de Gaffiot e Oxford Latin Dictionary), constituindo-se, dessa forma, como um decalque da língua latina; ou 2 - <i>angulado</i> pode ser analisado como derivado do substantivo <i>ângulo</i> com o sufixo -<i>ado</i>, visto que o substantivo <i>ângulo</i> teve, segundo o Dicionário Houaiss, sua primeira atestação no século XIV; portanto, no momento da elaboração do dicionário de Vandelli, há a possibilidade de o autor ter utilizado o recurso da própria língua portuguesa para introduzir a palavra na língua através do processo de derivação."@pt .

dicbio:form_angulada a ontolex:Form ;
    rdfs:label "feminino singular"@pt ;
    ontolex:writtenRep "angulada"@pt .

dicbio:form_angulado a ontolex:Form ;
    ontolex:writtenRep "angulado"@pt .

dicbio:form_angulados a ontolex:Form ;
    rdfs:label "masculino plural"@pt ;
    ontolex:writtenRep "angulados"@pt .

dicbio:etymon_angulatus a etym:Etymon ;
    dcterms:language "latim" ;
    dcterms:source "Gaffiot/OLD" ;
    ontolex:writtenRep "angulatus" .

dicbio:sense_angulado_1 a ontolex:LexicalSense ;
    skos:definition "Que apresenta ângulo; anguloso. "@pt .

