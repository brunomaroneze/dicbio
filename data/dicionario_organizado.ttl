### --- VERBETE: PERICARPO ---

dicbio:entry_pericarpo a ontolex:LexicalEntry ;
    etym:etymology dicbio:etym_pericarpo ;
    dcterms:created "2022-12-20"^^xsd:date ;
    dcterms:creator author:bruno_maroneze,
        author:raissa_buss ;
    lexinfo:gender lexinfo:masculine ;
    lexinfo:partOfSpeech lexinfo:noun ;
    ontolex:canonicalForm dicbio:form_pericarpo ;
    ontolex:otherForm dicbio:form_pericarpio ;
