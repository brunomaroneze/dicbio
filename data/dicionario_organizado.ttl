    dcterms:language "não se aplica" ;
    dcterms:source "“Critica Sacra”, de Edward Legh, 1639 (disponível em https://books.google.pt/books?id=0IRmAAAAcAAJ" ;
    ontolex:writtenRep "não se aplica" .

