    dcterms:language "latim científico" ;
    dcterms:source "tradução comentada das obras de Hipócrates e Galeno, escrita por René Chartier e publicada em 1679 (https://www.google.com.br/books/edition/Hippocratis_coi_et_Claudii_Galeni_Opera/Q83btvVqeBoC" ;
    ontolex:writtenRep "metacarpus" .

dicbio:form_metacarpo a ontolex:Form ;
