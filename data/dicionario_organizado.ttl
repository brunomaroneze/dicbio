### --- VERBETE: INSETO ---

dicbio:entry_inseto a ontolex:LexicalEntry ;
    etym:etymology dicbio:etym_inseto ;
    dcterms:created "2022-11-21"^^xsd:date ;
    dcterms:creator author:bruno_maroneze ;
